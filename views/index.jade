html
	head
		title Atmos
		meta(charset="utf-8")
		meta(http-equiv="x-ua-compatible" content="IE=Edge")
		meta(name="viewport" content="width=device-width,initial-scale=1")
		link(type="text/css" rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/animate.css/3.5.2/animate.min.css")
		link(type="text/css" rel="stylesheet" href="/static/dist/css/bundle.min.css")
		link(href="https://fonts.googleapis.com/css?family=Audiowide" rel="stylesheet")
		link(href="https://fonts.googleapis.com/css?family=Philosopher" rel="stylesheet")
		link(href="https://fonts.googleapis.com/css?family=Audiowide|Forum" rel="stylesheet")
		link(href="https://fonts.googleapis.com/css?family=Galada" rel="stylesheet")
		link(href="https://fonts.googleapis.com/css?family=Monoton" rel="stylesheet")
	body
		div(id="player")
			audio(autoplay hidden)
				source(src="music.mp3" type="audio/mpeg")
		div(class="loader active")
		div(class="stats center animated zoomInDown" style="display: none ; visibility: hidden;")
		div(class="container error center fadeIn" style="display: none")
			span(class="error-icon icon-error")
			h1(class="error-title") Oops, there's a problem
			p(class="error-info")
			button(type="button" class="icon-rocket" onclick="top.location.reload( true )") Try again

		div(class="container content center animated fadeIn" style="display: none") 
			h1(style = "font-family: 'Monoton', cursive !important; font-size: 92px;") ATMOS 
			<br>
			p(style="font-family: 'Forum', cursive !important; letter-spacing: 0.75em;") INTO THE SIXTH DIMENSION
			h3(style="letter-spacing: 0.5em;") 26-28 OCTOBER
			br
			hr
			button(id="hyper-btn" class="icon-rocket")  
				span EVENTS 
		div(class="events center animated fadeIn" style="display: none")
			div(style="margin-top: 10%")
				h1
					i(style = "font-family: 'Galada', cursive !important; font-size: 45px;") Events
			br
			div(class = "event-list" style="float:left; position: relative; margin-left: 15%;")
				a(href="src/Events/Tech Expo-2018.pdf" download="" class="events-download-link") Tech Expo 2018
				br
				a(href="src/Events/Time versus Physics.pdf" download="" class="events-download-link") Time v/s Physics
				br  
				a(href="src/Events/Suit-Up.pdf" download="" class="events-download-link") Suit Up !
				br
				a(href="src/Events/SciTech Quiz.pdf" download="" class="events-download-link") Sci-Tech Quiz 
				br
				a(href="src/Events/RoboWars.pdf" download="" class="events-download-link") RoboWars
				br
				a(href="src/Events/Prodigal Designer.pdf" download="" class="events-download-link") Prodigal Designer
				br
				a(href="src/Events/Nostrum Vitae.pdf" download="" class="events-download-link") Nostrum Vitae
				br
				a(href="src/Events/N-Crypton.pdf" download="" class="events-download-link") N-Crypton
				br
				a(href="src/Events/miniGP-draft.docx.pdf" download="" class="events-download-link") Mini-GP
				br
				a(href="src/Events/MIDNIGHT AV QUIZ.pdf" download="" class="events-download-link") Midnight AB Quiz
				br
				a(href="src/Events/Manager's dilemma.pdf" download="" class="events-download-link") Manager's dilemma
				br
				a(href="src/Events/Law Follower.pdf" download="" class="events-download-link") Law Follower

			div(class = "event-list" style="float: right; position: relative; margin-right: 15%;")
				a(href="src/Events/GameOfDrones ATMOS'18.pdf" download="" class="events-download-link") Game of Drones
				br
				a(href="src/Events/Cubing-Atmosphere-Rules.pdf" download="" class="events-download-link") Cubing-Atmosphere
				br
				a(href="src/Events/CourtRoom.pdf" download="" class="events-download-link") CourtRoom
				br
				a(href="src/Events/codejam_problem_statement.pdf" download="" class="events-download-link") CodeJam
				br
				a(href="src/Events/CODE GOLF.pdf" download="" class="events-download-link") Code Golf
				br
				a(href="src/Events/capture the flag_problem_statement.pdf" download="" class="events-download-link") Capture the Flag
				br
				a(href="src/Events/beam it draft 2018.pdf" download="" class="events-download-link") Beam it!
				br
				a(href="src/Events/ARC Competition for ATMOS 2018.pdf" download="Predestination" class="events-download-link") Predestination
				br
				a(href="src/Events/Anatomy Of Murder.pdf" download="" class="events-download-link") Anatomy of Murder
				br
				a(href="src/Events/ALGOMANIAC.pdf" download="" class="events-download-link") ALGOMANIAC
				br
				a(href="src/Events/Alchemy ATMOS Event .pdf" download="Snitch In Time" class="events-download-link") Snitch in Time
				br 
				a(href="src/Events/Cataclysmic Deduction.pdf" download="Cataclysmic Deduction" class="events-download-link") Cataclysmic Deduction
				br
				br 
					
				div(style="position: relative; bottom: 100px; left: 40%")
				button(id="home" class="icon-rocket")
					span HOME

		div(class="share center animated zoomInUp" style="display: none")
			a(class="btn icon-twitter icon-trim" data-share="twitter" href="https://twitter.com/BITSAtmos")
				span Tweet 
			a(class="btn icon-facebook icon-trim" data-share="facebook"  href="https://www.facebook.com/bits.atmos") 
				span Share

		script(src="https://cdnjs.cloudflare.com/ajax/libs/three.js/r75/three.min.js")
